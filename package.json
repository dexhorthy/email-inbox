{
    "name": "my-agent",
    "version": "0.1.0",
    "private": true,
    "scripts": {

        "baml:generate": "baml-cli generate",

        "typecheck": "tsc --noEmit",

        "lint:fix": "biome check src/ --write",
        "lint:fix:unsafe": "biome check src/ --write --unsafe",
        "lint": "biome check src/",

        "fix": "bun run baml:generate && bun run lint:fix",
        "check": "bun run baml:generate && bun run typecheck && bun run lint",

        "test:baml": "baml-cli test",
        "test:unit": "bun run src/test/test.ts",
        "test:e2e": "bun run src/test/test-e2e.ts",
        "test": "bun run test:baml && bun run test:unit && bun run test:e2e",

        "dev": "bun run fix && bun run src/cli.ts process --num-records 1",

        "githooks": "echo 'bun run pre-push' > .git/hooks/pre-push && chmod +x .git/hooks/pre-push",

        "pre-push": "bun run check && bun run test"
    },
    "dependencies": {
        "@boundaryml/baml": "latest",
        "@humanlayer/sdk": "latest",
        "@types/turndown": "latest",
        "agentops": "latest",
        "commander": "^14.0.0",
        "dotenv": "latest",
        "express": "latest",
        "googleapis": "latest",
        "humanlayer": "latest",
        "open": "latest",
        "tsx": "latest",
        "turndown": "latest",
        "typescript": "latest"
    },
    "devDependencies": {
        "@biomejs/biome": "latest",
        "@types/express": "latest",
        "@types/node": "latest",
        "@typescript-eslint/eslint-plugin": "latest",
        "@typescript-eslint/parser": "latest",
        "eslint": "latest",
        "prettier": "latest",
        "supertest": "latest"
    }
}
