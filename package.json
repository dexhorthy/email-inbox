{
    "name": "my-agent",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "bun run src/cli.ts",
        "check:fix": "biome check src/ --write",
        "check": "tsc --noEmit && biome check src/",
        "build": "bun run check:fix && bun run baml:generate",
        "test": "bun run baml:test && bun run test.ts",
        "test:e2e": "bun run test-e2e.ts",
        "baml:generate": "baml-cli generate",
        "baml:test": "baml-cli test",
        "githooks": "echo 'bun run check && bun run test' > .git/hooks/pre-push && chmod +x .git/hooks/pre-push",
        "fmt:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
        "fmt": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
        "fix": "bun run fmt:fix && bun run check:fix"
    },
    "dependencies": {
        "@boundaryml/baml": "latest",
        "@humanlayer/sdk": "latest",
        "@types/turndown": "latest",
        "agentops": "latest",
        "commander": "^14.0.0",
        "dotenv": "latest",
        "express": "latest",
        "googleapis": "latest",
        "humanlayer": "latest",
        "open": "latest",
        "tsx": "latest",
        "turndown": "latest",
        "typescript": "latest"
    },
    "devDependencies": {
        "@biomejs/biome": "latest",
        "@types/express": "latest",
        "@types/node": "latest",
        "@typescript-eslint/eslint-plugin": "latest",
        "@typescript-eslint/parser": "latest",
        "eslint": "latest",
        "prettier": "latest",
        "supertest": "latest"
    }
}
